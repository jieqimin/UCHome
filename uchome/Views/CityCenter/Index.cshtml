        GetSchSubjectList('语文|03FCB8E0-478C-4FF3-99F4-A17CA6C0B6AE,数学|C9D9295B-41F3-4334-89EA-8956F06B3222,英语|C709FB4C-D39E-462E-86CF-8128059C383E,音乐|60AE8CDE-D644-451F-B262-A39DB070F292,体育|91E53D0B-0C94-4E7F-BEDA-7980C25EDE93,美术|7156A9CC-B5F9-4320-9EEB-AB1500CEE452,品德与生活|556A4D96-7B9D-4A53-B446-7BD75D899FB,品德与社会|8513A198-C039-4C92-B169-AB6FB175391F,科学|C125AE7E-C260-4B8E-973D-0B906AA01EA6,信息|B836A771-212E-4E50-82CC-CC9F0B848EFB', 'SubjectList');
        $("#teacherRooms").load("@Html.Raw(Url.Action("GoodStuAndTeaList", new { userType = "t", pageSize = 4 }))"); //优秀教师
        $("#studentRooms").load("@Html.Raw(Url.Action("GoodStuAndTeaList", new { userType = "s", pageSize = 4 }))"); //优秀学生
        //获取教案列表（课件）
        GetSchSubjectList('语文|03FCB8E0-478C-4FF3-99F4-A17CA6C0B6AE,数学|C9D9295B-41F3-4334-89EA-8956F06B3222,英语|C709FB4C-D39E-462E-86CF-8128059C383E,音乐|60AE8CDE-D644-451F-B262-A39DB070F292,体育|91E53D0B-0C94-4E7F-BEDA-7980C25EDE93,美术|7156A9CC-B5F9-4320-9EEB-AB1500CEE452,品德与生活|556A4D96-7B9D-4A53-B446-7BD75D899FB,品德与社会|8513A198-C039-4C92-B169-AB6FB175391F,科学|C125AE7E-C260-4B8E-973D-0B906AA01EA6,信息|B836A771-212E-4E50-82CC-CC9F0B848EFB', 'LessonPlanSubj');
        $("#datastatic").load("@Html.Raw(Url.Action("DataStatics", "Query"))"); //优秀文章展示下面的统计数量
        GetArticlesByArea();
        GetSpaceAlivesByArea();
        GetSumResource();//获取资源总数
        GetEbooksByArea();//分区获取电子书包的数量
        GetDegreesByArea();//活跃度
    });

    //获取科目列表（资源）
    function GetSchSubjectList(subjects, id) {
        $("#" + id).html("");
        var subjectInfo = subjects.split(',');
        var subHtml = "<tr>";
        var k = 1;
        for (var j = 0; j <= subjectInfo.length; j++) {
            if (k < 3) {
                if (subjectInfo[j] != undefined)
                    var subjectNameAndId = subjectInfo[j].split('|');
                var subName = subjectNameAndId[0];
                var subId = subjectNameAndId[1];
                if (id == "SubjectList") {
                    subHtml += "<td onclick=\"GetScourseBySubId('" + subName + "','" + subId + "')\"><a href=\"#" + subName + "\" id=\"" + subId + "\" class=\"box\" style=\"color:white\" >" + subName + "</a></td>";

                } else {
                    subHtml += "<td onclick=\"GetLessonPlanBySubId('" + subName + "','" + subId + "')\"><a href=\"#" + subName + "\" id=\"" + subId + "\" class=\"box\" style=\"color:white\" >" + subName + "</a></td>";
                }

                k++;
            } else {
                subHtml += "</tr>";
                $("#" + id).append(subHtml);
                subHtml = "<tr>";
                k = 1;
                j--;
            }
        }
        if (id == "SubjectList") {
            var subjectInformation = subjectInfo[0].split('|');
            GetScourseBySubId(subjectInformation[0], subjectInformation[1]);
        } else {
            var subjectInfor = subjectInfo[0].split('|');
            GetLessonPlanBySubId(subjectInfor[0], subjectInfor[1]);
        }
    }


    //获取资源
    function GetScourseBySubId(subName, subId) {
        $(".TabList").html("");
        var conditionType = "Subject";
        var condition = subId;
        var format = "";
        var type = "";
        var page = "1";
        var getSubUrl = "@Url.Action("GetResource", "CityCenter")?conditionType=" + conditionType + "&condition=" + condition + "&format=" + format
                + "&type=" + type + "&page=" + page;
        $.ajax({
            url: getSubUrl,
            type: "post",
            success: function (result) {
                var resultData = eval("(" + eval("(" + result + ")") + ")");
                var scourseOneHtml = " <ul id=\"" + subName + "\" class=\"TabMain\" style=\"display: block;\">";
                $.each(resultData.ResourceList, function (i, item) {
                    if (i <= 4) {
                        var resourceName = item.ResourceName;
                        var link = item.LinkUrl; //链接地址
                        var image = item.CoverUrl;
                        if (image == "" || image == null) {
                            scourseOneHtml += "<li><a href=\"#\"><img src=\"@Url.Content("~/Content/CityCenter/images/img.jpg")\" /></a><h4>" + resourceName + "</h4></li>";
                        } else {
                            scourseOneHtml += "<li><a href=\"#\"><img src=\"" + image + "\"  style=\"width: 118px; height: 168px;\"/></a><h4>" + resourceName + "</h4></li>";
                        }
                    }

                });
                scourseOneHtml += "</ul>";
                $(".TabList").append(scourseOneHtml);
            }

        });
        $(".Subject table tbody tr td").click(function () {
            $(".TabMenu li").removeClass("active"); //Remove any "active" class
            $(this).addClass("active"); //Add "active" class to selected tab
            $(".TabMain").hide(); //Hide all tab content
            var activeTab = $(this).find("a").attr("href"); //Find the rel attribute value to identify the active tab + content
            $(activeTab).fadeIn(); //Fade in the active content
            return false;
        });
    }

