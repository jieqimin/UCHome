@{    Layout = null;}<!DOCTYPE html><html><head><meta charset="UTF-8"><meta name="viewport" content="width=device-width,initial-scale=1,minimum-scale=1,maximum-scale=1,user-scalable=no" /><title>校历查询</title><link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/css/h5/reset.css")" /><link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/css/h5/public.css")" /><script src="@Url.Content("~/Content/js/jquery.min.js")"></script><script src="@Url.Content("~/Scripts/public.js")"></script><style></style></head><body><header><p>校历查询</p></header><section><ul class="List" id="schoolcalendar"></ul></section><script>        $(function () { $.ajax({ url: "@Url.Action("GetCurrSchoolCalendar")", success: function (data) { $("#schoolcalendar").html(""); var result = eval("(" + data + ")"); var curweek = 1; var curdate = new Date(); $.each(result, function (index, item) { var xnweek = item.XNWeek; var startdate = new Date(item.StartDate).toLocaleDateString(); var enddate = new Date(item.EndDate).toLocaleDateString(); var memo = item.Memo; var html; if (curdate >= new Date(item.StartDate) && curdate <= new Date(item.EndDate)) { curweek = xnweek - 1; html = "<li class='Current' id='week" + xnweek + "'><a><p class='Title'><span datatime1='" + startdate + "' datatime2='" + enddate + "'>第" + xnweek + "周：" + startdate + "~" + enddate + "</span></p><p class='SubTitle'>" + memo + "</p></a></li>"; $("#schoolcalendar").append(html); } else { html = "<li id='week" + xnweek + "'><a><p class='Title'><span datatime1='" + startdate + "' datatime2='" + enddate + "'>第" + xnweek + "周：" + startdate + "~" + enddate + "</span></p><p class='SubTitle'>" + memo + "</p></a></li>"; $("#schoolcalendar").append(html); } }); var currenturl = document.location.href; if (currenturl.indexOf("#week") < 0) { document.location.href = document.location.href + "#week" + curweek; } else { document.location.href = currenturl; } } }); })</script></body></html>