@model IEnumerable<SchInfo.Model.Dictionary>

@{
}
<link rel="stylesheet" href="@Url.Content("~/plugs/datatables/css/jquery.dataTables.css")">
<link rel="stylesheet" href="@Url.Content("~/plugs/datatables/css/dataTables.bootstrap.css")">
<link rel="stylesheet" href="@Url.Content("~/plugs/scrollbar/css/sui.css")">
<script src="@Url.Content("~/plugs/datatables/js/jquery.datatables.js")"></script>
<script src="@Url.Content("~/plugs/datatables/js/dataTables.bootstrap.js")"></script>
<script src="@Url.Content("~/plugs/datatables/js/Jquery.dataTableSetting.js")"></script>
<div class="pagew form-position">
    <div class="searchbar form-inline">
        <div class="form-group">
            <label for="exampleInputName2">字典类型</label>
            <input type="text" class="form-control" id="search_dicttype" placeholder="请输入类型">
        </div>
        <div class="form-group">
            <label for="exampleInputEmail2">字典名称</label>
            <input type="text" class="form-control" id="search_dictname" placeholder="输入关键字模糊查询">
        </div>
        <button type="button" id="btn-searchdictionary" class="btn btn-primary">搜索</button>
        <button type="button" class="btn btn-info" onclick="RedircetUrl('@Url.Action("DictionaryCreate","SchoolInfo")')">创建新字典</button>
    </div>
    <hr class="divider" />
    
    <table class="table  table-striped table-bordered table-hover datatable" id="table_dictionary">
        <thead>
            <tr>
                <th>
                    代码
                </th>
                <th>
                    名称
                </th>
                <th>
                    类型
                </th>
                <th>
                    排序
                </th>
                <th>
                    状态
                </th>
                <th>
                    操作
                </th>
            </tr>
        </thead>
        <tfoot>
            <tr><th colspan="6">共<label id="records"></label> 项，当前显示前100项记录，如未找到你想要的结果，可以将筛选条件调整更精确！</th></tr>
        </tfoot>
    </table>
</div>
<script>
    $.get("@Url.Action("GetDictTypes")", function (data) {
    $("#search_dicttype").autocomplete({
        source: data
    });
    })

</script>
<script>
    var initlisturl = "@Url.Action("GetDictionaryList")";
    var operurl_delete = "@Url.Action("DeleteDictionary")";
        var operurl_edit = "@Url.Action("DictionaryEdit")";
    var operurl_recovery = "@Url.Action("RescoverDictionary")";
</script>
<script src="@Url.Content("~/Data/SchoolInfo/Dictionary/dictionary.js")"></script>